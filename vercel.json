{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                }
            ]
        }
    ],
    "rewrites": [{ "source": "/api/renderings/", "destination": "http://haeley-datacubes.bakoe.dev:8000/renderings/" }]
}
